{
  "name": "node-package-starter",
  "version": "0.0.0-dev",
  "license": "MIT",
  "description": "",
  "main": "dist",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "mocha -r ts-node/register/transpile-only src/**/*.spec.ts",
    "test:w": "mocha -r ts-node/register/transpile-only --watch-extensions ts --watch src/**/*.spec.ts",
    "test:cov": "nyc mocha src/**/*.spec.ts",
    "test:d": "node --inspect-brk -r ts-node/register/transpile-only node_modules/mocha/bin/_mocha --no-timeouts --watch-extensions ts --watch src/**/*.spec.ts",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "tsc --noEmit --strict --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals --noUnusedParameters",
    "tsclint:w": "npm run tsclint -- --watch",
    "eslint": "eslint src --ext ts",
    "eslint:fix": "eslint src --ext \"ts\" --fix",
    "eslint:w": "watch \"eslint src --ext ts\" src",
    "lint:w": "run-p tsclint:w eslint:w",
    "semantic-release": "semantic-release",
    "prepublishOnly": "sed -i -e 's/devDependencies/_devDependencies/g' package.json",
    "copysource": "cp -rv src dist && find dist/src -name '*.spec.ts' | xargs rm -f",
    "tsc": "tsc",
    "_release:node": "tsc && semantic-release",
    "_release:browser": "ng-packagr && semantic-release",
    "_build:browser": "ng-packagr",
    "_build:node": "tsc",
    "prebuild": "npm run clean",
    "clean": "rimraf dist",
    "commit": "git-cz"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/unlight/node-package-starter.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@angular/compiler": "^7.2.0",
    "@angular/compiler-cli": "^7.2.0",
    "@angular/core": "^7.2.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/npm": "^5.1.3",
    "@types/expect": "1.20.3",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "eslint": "^5.12.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-tslint": "^3.1.0",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-unicorn": "^7.0.0",
    "expect": "1.20.2",
    "mocha": "^5.2.0",
    "ng-packagr": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.3",
    "ts-node": "^7.0.1",
    "tsickle": "^0.34.0",
    "tslint": "^5.12.1",
    "tslint-clean-code": "^0.2.9",
    "tslint-microsoft-contrib": "^6.0.0",
    "tslint-sonarts": "^1.8.0",
    "typescript": "~3.2.2",
    "typescript-eslint-parser": "^21.0.2",
    "watch": "^1.0.2"
  }
}
