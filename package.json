{
  "name": "node-package-starter",
  "version": "0.0.0-dev",
  "license": "MIT",
  "description": "",
  "author": "2020",
  "keywords": [],
  "main": "./cjs/index.js",
  "module": "./mjs/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./mjs/index.mjs",
      "require": "./cjs/index.js"
    }
  },
  "scripts": {
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "npm run mocha -- src/**/*.spec.ts",
    "test:cov": "c8 --reporter text --exclude \"**/*.spec.ts\" --exclude \"**/testing/**\"  npm run test:r -- --no-timeouts",
    "test:w": "npm run test:r -- --watch-files src --watch",
    "lint": "npm run eslint && npm run tscheck",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "lint:fix": "npm run eslint:fix && npm run lint:doc:fix",
    "lint:doc": "sh Taskfile remark_run",
    "lint:doc:fix": "sh Taskfile remark_run --output",
    "format:src": "prettier src --write",
    "commit_lint": "sh Taskfile commit_lint",
    "commit": "cz",
    "semantic-release": "semantic-release",
    "dev": "node node_modules/webpack-dev-server/bin/webpack-dev-server",
    "build": "sh Taskfile build_hybrid",
    "lint:w": "run-p tscheck:w eslint:w",
    "test:m": "node node_modules/@stryker-mutator/core/bin/stryker run",
    "mocha": "node -r tsconfig-paths/register -r @swc/register node_modules/mocha/bin/_mocha",
    "test:d": "ndb -r @swc/register node_modules/mocha/bin/_mocha --no-timeouts --watch-files src/**/*.ts --watch src/**/*.spec.ts"
  },
  "config": {
    "ghooks": {
      "pre-commit": "precise-commits",
      "commit-msg": "sh Taskfile commit_lint",
      "pre-push": "npm run test"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "1.2.162",
    "@swc/helpers": "0.3.8",
    "@swc/jest": "0.2.20",
    "@swc/register": "^0.1.10",
    "@types/mocha": "^9.1.0",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "c8": "^7.11.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-regexp": "^1.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "42.0.0",
    "eslint-plugin-wix-editor": "^3.3.0",
    "expect": "^27.5.1",
    "ghooks": "^2.0.4",
    "git-branch-is": "^4.0.0",
    "jest": "^27.5.1",
    "mocha": "9.2.2",
    "npm-run-all": "^4.1.5",
    "precise-commits": "^1.0.2",
    "prettier": "^2.6.1",
    "reflect-metadata": "^0.1.13",
    "remark": "^14.0.2",
    "remark-cli": "^10.0.1",
    "remark-license": "^6.1.0",
    "remark-toc": "^8.0.1",
    "request": "^2.88.2",
    "semantic-release": "^19.0.2",
    "semantic-release-monorepo": "^7.0.5",
    "simplytyped": "^3.3.0",
    "tsconfig-paths-jest-mapper": "^1.4.0",
    "tslib": "^2.3.1",
    "tsup": "^5.12.2",
    "typescript": "4.6.3",
    "watchexec-bin": "^1.0.0"
  }
}
