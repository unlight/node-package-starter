{
  "name": "node-package-starter",
  "version": "0.0.0-dev",
  "license": "MIT",
  "description": "",
  "author": "2020",
  "keywords": [],
  "type": "commonjs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/ghooks/bin/module-install",
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "npm run mocha -- src/**/*.spec.ts",
    "test:cov": "c8 --reporter text --exclude \"**/*.spec.ts\" --exclude \"**/testing/**\" npm run test:r -- --no-timeouts",
    "test:w": "npm run test:r -- --watch-files src --watch",
    "lint": "npm run eslint && npm run tscheck",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "lint:fix": "npm run eslint:fix && npm run lint:doc:fix",
    "lint:doc": "sh Taskfile remark_run",
    "lint:doc:fix": "sh Taskfile remark_run --output",
    "format:src": "prettier src --write",
    "commit_lint": "sh Taskfile commit_lint",
    "commit": "cz",
    "dev": "node node_modules/webpack-dev-server/bin/webpack-dev-server",
    "build": "sh Taskfile build_microbundle1",
    "lint:w": "run-p tscheck:w eslint:w",
    "test:m": "node node_modules/@stryker-mutator/core/bin/stryker.js run",
    "mocha": "node -r ts-node/register node_modules/mocha/bin/mocha",
    "test:d": "ndb -r ts-node/register node_modules/mocha/bin/mocha src/**/*.spec.ts --watch-files src --no-timeouts --watch"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "config": {
    "ghooks": {
      "commit-msg": "sh Taskfile commit_lint",
      "pre-push": "npm run test"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/compat": "^1.2.0",
    "@eslint/js": "^9.12.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc/core": "^1.7.36",
    "@swc/helpers": "^0.5.13",
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.7.5",
    "c8": "^10.1.2",
    "commitizen": "^4.3.1",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-perfectionist": "^3.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-wix-editor": "^3.3.0",
    "expect": "^29.7.0",
    "ghooks": "^2.0.4",
    "git-branch-is": "^4.0.0",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "mocha": "^10.7.3",
    "node-watchexec": "github:longdog/node-watchexec",
    "npm-run-all": "^4.1.5",
    "pkgroll": "^2.5.0",
    "prettier": "^3.3.3",
    "reflect-metadata": "^0.2.2",
    "remark": "^15.0.1",
    "remark-cli": "^12.0.1",
    "remark-license": "^6.1.0",
    "remark-toc": "^9.0.0",
    "semantic-release": "^24.1.2",
    "semantic-release-monorepo": "^8.0.2",
    "simplytyped": "^3.3.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-jest-mapper": "^1.4.0",
    "tslib": "^2.8.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0"
  }
}
