{
  "name": "node-package-starter",
  "version": "0.0.0",
  "description": "Node package starter",
  "license": "ISC",
  "main": "lib/index.js",
  "typings": "src/index.ts",
  "scripts": {
    "test": "npm run eslint && npm run tscheck && npm run t",
    "t": "npm run mocha -- src/*.spec.ts",
    "mocha": "ts-node -F node_modules/mocha/bin/_mocha",
    "test:w": "npm run mocha -- --watch-extensions ts --watch src/**/*.spec.ts",
    "test:d": "node --inspect-brk node_modules/ts-node/dist/_bin.js -F node_modules/mocha/bin/_mocha --no-timeouts src/**/*.spec.ts",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "npm run tsc -- --noEmit --pretty false --strictNullChecks --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals",
    "tsclint:w": "npm run tsclint -- --watch",
    "eslint": "eslint --ext ts src",
    "eslint:fix": "eslint src --ext \"ts\" --fix",
    "lint:w": "concurrently -- \"npm run tsclint:w\" \"npm run eslint:w\"",
    "clean": "rimraf lib"
  },
  "dependencies": {
    "tslib": "^1.8.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.47",
    "concurrently": "^3.5.0",
    "eslint": "^4.10.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-typescript": "^0.8.0",
    "eslint-plugin-unicorn": "^2.1.2",
    "mocha": "^4.0.1",
    "rimraf": "^2.6.2",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.1",
    "typescript-eslint-parser": "^8.0.1"
  }
}
